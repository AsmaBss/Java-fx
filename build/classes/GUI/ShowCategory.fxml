<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="760.0" styleClass="mainFxmlClass" stylesheets="@../css/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ShowCategoryController">
    
   <children>
      <ImageView fitHeight="507.0" fitWidth="760.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/dashboard.png" />
         </image>
      </ImageView>
      <Label layoutX="150.0" layoutY="121.0" prefHeight="42.0" prefWidth="91.0" text="CATEGORIES">
         <font>
            <Font name="Traditional Arabic" size="25.0" />
         </font>
      </Label>
      <TableView fx:id="categories" layoutX="235.0" layoutY="176.0" prefHeight="262.0" prefWidth="381.0">
        <columns>
          <TableColumn fx:id="libelle" prefWidth="379.0" text="LIBELLE" />
        </columns>
      </TableView>
      <Button layoutX="688.0" layoutY="460.0" mnemonicParsing="false" onAction="#deleteCategorie" prefHeight="35.0" prefWidth="64.0" text="Delete" />
      <Button fx:id="updateC" layoutX="616.0" layoutY="460.0" mnemonicParsing="false" onAction="#updateCategorie" prefHeight="35.0" prefWidth="64.0" text="Update">
         <font>
            <Font name="Sylfaen" size="13.0" />
         </font></Button>
      <Button fx:id="btnJob" layoutX="318.0" layoutY="54.0" mnemonicParsing="false" onAction="#showJob" prefHeight="25.0" prefWidth="77.0" text="Job Offers" />
      <Button fx:id="btnIntenship" layoutX="174.0" layoutY="54.0" mnemonicParsing="false" onAction="#showInternship" text="Internship Offers" />
      <Button fx:id="btnJobFront" layoutX="306.0" layoutY="21.0" mnemonicParsing="false" onAction="#showJobFront" prefHeight="25.0" prefWidth="105.0" text="Job Offers Front" />
      <Button fx:id="btnIntenshipFront" layoutX="159.0" layoutY="21.0" mnemonicParsing="false" onAction="#showInternFront" text="Internship Offers Front" />
      <Button fx:id="btnCategory" layoutX="270.0" layoutY="83.0" mnemonicParsing="false" onAction="#showCategory" text="Category" />
      <Button fx:id="btnAdd" layoutX="692.0" layoutY="121.0" mnemonicParsing="false" onAction="#addCategory" prefHeight="35.0" prefWidth="54.0" text="Add" />
   </children>
</AnchorPane>
