      <Button fx:id="btnJobFront" layoutX="307.0" layoutY="13.0" mnemonicParsing="false" onAction="#showJobFront" prefHeight="25.0" prefWidth="105.0" text="Job Offers Front" />
      <Button fx:id="btnIntenshipFront" layoutX="160.0" layoutY="13.0" mnemonicParsing="false" onAction="#showInternFront" text="Internship Offers Front" />
      <Button fx:id="btnCategory" layoutX="271.0" layoutY="81.0" mnemonicParsing="false" onAction="#showCategory" text="Category" />
   </children>
</AnchorPane>
