<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="750.0" stylesheets="@../css/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.UpdateCategoryController">
