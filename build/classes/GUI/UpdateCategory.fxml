      </ImageView>
      <Label contentDisplay="TEXT_ONLY" graphicTextGap="5.0" layoutX="160.0" layoutY="118.0" lineSpacing="4.0" prefHeight="36.0" prefWidth="209.0" text="Update Category" textAlignment="CENTER" textOverrun="CLIP" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="118.0">
         <font>
            <Font size="24.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Label>
      <Label layoutX="353.0" layoutY="225.0" text="Libelle">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="txtLib" layoutX="353.0" layoutY="249.0" prefHeight="33.0" prefWidth="170.0" promptText="Libelle *">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="btnUpdateCategory" layoutX="468.0" layoutY="300.0" mnemonicParsing="false" onAction="#updateCateg" text="Update">
